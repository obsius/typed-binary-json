{
	"name": "typed-binary-json",
	"version": "1.0.8",
	"description": "A TBJSON parser and serializer.",
	"author": "Jeff Seaman",
	"contributors": [
		"Jeff Seaman <mr.jeffrey.seaman@gmail.com> (https://www.ngc6822.com)"
	],
	"license": "MIT",
	"main": "index.js",
	"scripts": {
		"prepare": "rollup -c",
		"start": "nodemon --inspect dev.js --exec babel-node",
		"benchmarks": "babel-node benchmarks.js"
	},
	"babel": {
		"plugins": [
			"@babel/plugin-proposal-class-properties"
		],
		"presets": [
			[
				"@babel/preset-env",
				{
					"targets": {
						"node": "current"
					}
				}
			]
		]
	},
	"devDependencies": {
		"@babel/core": "^7.4.0",
		"@babel/node": "^7.2.2",
		"@babel/plugin-proposal-class-properties": "^7.4.0",
		"@babel/preset-env": "^7.4.2",
		"nodemon": "^1.18.10",
		"rollup": "^1.7.3",
		"rollup-plugin-babel": "^4.3.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/obsius/typed-binary-json.git"
	},
	"keywords": [
		"json",
		"binary",
		"typed",
		"parser",
		"serializer"
	]
}
